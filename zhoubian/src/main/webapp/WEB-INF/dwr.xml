<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 2.0//EN" "http://getahead.org/dwr/dwr20.dtd">

<dwr>
	<allow>
		<create creator="new" javascript="category">
			<param name="class" value="net.zhoubian.app.web.dwr.CategoryCombox" />
		</create>

		<!-- this is a bad idea for live, but can be useful in testing -->
		<convert converter="exception" match="java.lang.Exception" />
		<convert converter="bean" match="java.lang.StackTraceElement" />
		
		<create creator="spring" scope="application" javascript="Chat">
			<param name="beanName" value="tmessageAction" />
		</create>
		<convert converter="bean" match="net.zhoubian.app.model.TMessage" />
	</allow>
</dwr>
